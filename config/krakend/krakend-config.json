      "output_encoding": "json",
      "input_headers": ["*"],
      "backend": {
        "url_pattern": "/api/request/manage",
        "encoding": "json",
        "host": ["http://10.43.100.29:3001"],
        "disable_tls_verify": true
      },
      "method": "POST"
