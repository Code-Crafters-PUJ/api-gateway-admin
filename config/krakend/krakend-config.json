{
  "version": 3,
  "port": 8081,
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": ["*"],
      "allow_methods": ["GET", "POST", "PUT", "DELETE"],
      "allow_headers": ["*"],
      "expose_headers": ["*"],
      "max_age": "12h",
